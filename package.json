{
  "name": "afiye",
  "version": "1.0.0",
  "description": "Afiye master repository",
  "private": true,
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "start": "node ./dist/server.js",
    "buildDev": "rm -rf dist && webpack --mode development --config webpack/webpack.server.js && webpack --config webpack/webpack.dev.js",
    "buildProd": "rm -rf dist && webpack --mode production --config webpack/webpack.server.js && webpack --config webpack/webpack.prod.js",
    "start-w": "webpack serve --open --config webpack/webpack.dev.js",
    "build-o": "webpack --config webpack/webpack.prod.js",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IDM-SP-2021/afiye.git"
  },
  "keywords": [],
  "author": "",
