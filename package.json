  "bugs": {
    "url": "https://github.com/IDM-SP-2021/afiye/issues"
  },
  "homepage": "https://github.com/IDM-SP-2021/afiye#readme",
  "dependencies": {
    "d3": "^6.3.1",
    "d3-force": "^2.1.1",
    "d3-zoom": "^2.0.0",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "neo4j-driver": "^1.7.7"
  },
